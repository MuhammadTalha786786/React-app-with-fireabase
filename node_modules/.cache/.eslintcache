n                placeholder='Email'\n                InputProps={{\n                  startAdornment: (\n                    <InputAdornment position='start'>\n                      <EmailIcon />\n                    </InputAdornment>\n                  ),\n                }}\n              />\n\n              <Input\n                value={password}\n                setValue={setPassword}\n                variant='outlined'\n                name='password'\n                type={showPassword ? 'text' : 'password'}\n                fullWidth\n                margin='normal'\n                placeholder='Password'\n                InputProps={{\n                  endAdornment: (\n                    <InputAdornment position='end'>\n                      <IconButton\n                        aria-label='toggle password visibility'\n                        onClick={() => {\n                          setShowPassword(!showPassword);\n                        }}\n                      >\n                        {showPassword ? (\n                          <VisibilityIcon />\n                        ) : (\n                          <VisibilityOffIcon />\n                        )}\n                      </IconButton>\n                    </InputAdornment>\n                  ),\n                }}\n              />\n\n              <Input\n                value={userNumber}\n                setValue={setUserNumber}\n                variant='outlined'\n                name='phone-number'\n                type='number'\n                fullWidth\n                margin='normal'\n                placeholder='number'\n                maxLength={2}\n                InputProps={{\n                  inputMode: 'numeric',\n                  maxLength: 12,\n                  pattern: '[0-9]*',\n                  startAdornment: (\n                    <InputAdornment position='start'>\n                      <PhoneIcon />\n                    </InputAdornment>\n                  ),\n                }}\n              />\n\n              <div className='my-3'>\n                <Button\n                  text={loading ? <CircularProgress size={20} /> : 'Register'}\n                  type='button'\n                  onClick={RegiserUser}\n                  style={{\n                    backgroundColor: StyleGuide.color.color3,\n                    color: StyleGuide.color.color5,\n                  }}\n                  class='button1 btn btn-lg btn-block fontFamily w-100'\n                />\n              </div>\n\n              <Grid container>\n                <Grid item xs>\n                  <Link href='#' variant='body2'>\n                    <p className='fontFamily extafont'>Forgot password?</p>\n                  </Link>\n                </Grid>\n                <Grid item>\n                  <Link to='/UserLogin'>\n                    <p className='fontFamily extafont'>\n                      Already have an account? Login Here\n                    </p>\n                  </Link>\n                </Grid>\n              </Grid>\n            </Box>\n          </Box>\n\n          <div>\n            <Snackbar open={open} autoHideDuration={6000} onClose={handleClose}>\n              <Alert\n                onClose={handleClose}\n                severity={isErrorMessage ? 'success' : 'error'}\n                sx={{ width: '100%' }}\n              >\n                {message}\n              </Alert>\n            </Snackbar>\n          </div>\n        </Container>\n      </ThemeProvider>\n    </>\n  );\n};\n\nexport default RegisterUser;\n","/Users/codeninja/Desktop/React-app-with-fireabase/src/Utils/GoogleSignIn.js",["106","107","108","109"],"/Users/codeninja/Desktop/React-app-with-fireabase/src/Pages/UserPanel/Home/UserHome.js",["110"],{"ruleId":"111","replacedBy":"112"},{"ruleId":"113","replacedBy":"114"},{"ruleId":"115","severity":1,"message":"116","line":3,"column":32,"nodeType":"117","messageId":"118","endLine":3,"endColumn":38},{"ruleId":"11