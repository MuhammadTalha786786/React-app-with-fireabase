hen((snapshot) => {\n        const users = [];\n        snapshot.forEach((user) => {\n          const data = user.data();\n          users.push(data);\n        });\n        this.setState({ users: users });\n      })\n      .catch((error) => {\n        console.log(error);\n      });\n  }\n  render() {\n    return (\n      <>\n        <h3 className='text-center text-primary'>Added Users</h3>\n\n        <br />\n\n        <div class='table-responsive-sm'>\n          <table class='table table-responsive table-hover table-bordered table-striped table-dark'>\n            <thead>\n              <tr>\n                <th scope='col'>Name</th>\n                <th scope='col'>Email</th>\n                <th scope='col'>Cell Number</th>\n                <th scope='col'>Age</th>\n              </tr>\n            </thead>\n\n            {this.state.users &&\n              this.state.users.map((user) => {\n                return (\n                  <tbody>\n                    <tr>\n                      <th>{user.name}</th>\n                      <td>{user.Email}</td>\n                      <td>{user.Cellnumber}</td>\n                      <td>{user.age}</td>\n                    </tr>\n                  </tbody>\n                );\n              })}\n          </table>\n        </div>\n      </>\n    );\n  }\n}\nexport default ViewUser;\n","import './App.css';\nimport Header from './Components/Header';\nimport { HashRouter as Router, Switch, Route } from 'react-router-dom';\nimport AddUser from './Components/AddUser';\nimport ViewUser from './Components/ViewUser';\nimport { Link } from 'react-router-dom';\n\nfunction App() {\n  return (\n    <Router basename={process.env.PUBLIC_URL}>\n      <Header />\n      <Switch>\n        <Route\n          exact\n          path='/'\n          render={() => {\n            return (\n              <>\n                <h1 className='heading shadow mt-5 p-3 mb-5 bg-info rounded'>\n                  React JS App using Firebase\n                